{
    "name": "ecodev/my-ichtus",
    "description": "My Ichtus",
    "type": "project",
    "homepage": "https://github.com/ecodev/my-ichtus",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "check": [
            "php-cs-fixer fix --ansi --dry-run --diff",
            "@test"
        ],
        "fix": [
            "php-cs-fixer fix --ansi"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "serve": "php -S 0.0.0.0:8080 -t htdocs/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    },
    "extra": {
        "zf": {
            "component-whitelist": [
                "zendframework/zend-expressive",
                "zendframework/zend-expressive-helpers",
                "zendframework/zend-expressive-router",
                "zendframework/zend-httphandlerrunner",
                "zendframework/zend-expressive-fastroute"
            ]
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "cakephp/chronos": "^1.2",
        "dasprid/container-interop-doctrine": "^1.1",
        "doctrine/migrations": "^1.8",
        "ecodev/graphql-doctrine": "^6.0",
        "zendframework/zend-component-installer": "^2.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-diactoros": "^2.0",
        "zendframework/zend-expressive": "^3.2",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-helpers": "^5.1",
        "zendframework/zend-expressive-session": "^1.2",
        "zendframework/zend-expressive-session-ext": "^1.3",
        "zendframework/zend-permissions-acl": "^2.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.4",
        "zendframework/zend-expressive-tooling": "^1.0",
        "zfcampus/zf-development-mode": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "server/Application/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "tests/ApplicationTest/"
        }
    }
}
