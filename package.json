        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.3",
        "prettier": "2.0.5",
        "protractor": "~5.4.0",
        "ts-node": "~8.8.2",
        "tslint": "^6.1.1",
        "typescript": "~3.8.3"
    }
