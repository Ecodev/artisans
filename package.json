  "scripts": {
    "ng": "ng",
    "ng-hm": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "dev-aot": "yarn codegen && yarn run ng-hm serve --prod",
    "dev": "yarn codegen && ng serve",
    "prod": "yarn codegen && ng build --prod",
    "test": "yarn codegen && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts"
  },
  "lint-staged": {
    "client/**/*.ts": [
      "node ./ng-lint-staged.js lint",
      "git add"
    ]
  },
