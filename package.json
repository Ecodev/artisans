{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "client-config": "`which php8.1 || which php` ./bin/generate-client-configuration.php",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn client-config && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build && ng run artisans:server:production && find data/tmp/build -type f -not -name '*.gz' -exec gzip --keep --best {} \\; && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "dependencies": {
        "@angular/animations": "~13.2.5",
        "@angular/cdk": "~13.2.5",
        "@angular/common": "~13.2.5",
        "@angular/compiler": "~13.2.5",
        "@angular/core": "~13.2.5",
        "@angular/flex-layout": "^13.0.0-beta.38",
        "@angular/forms": "~13.2.5",
        "@angular/localize": "^13.2.5",
        "@angular/material": "^13.2.5",
        "@angular/platform-browser": "~13.2.5",
        "@angular/platform-browser-dynamic": "~13.2.5",
        "@angular/platform-server": "^13.2.5",
        "@angular/router": "~13.2.5",
        "@apollo/client": "^3.4.16",
        "@ecodev/fab-speed-dial": "^10.0.0",
        "@ecodev/natural": "^42.4.1",
        "@ecodev/natural-editor": "^42.4.1",
        "@graphql-tools/mock": "^8.4.0",
        "@nguniversal/express-engine": "^13.0.2",
        "apollo": "^2.33.6",
        "apollo-angular": "^3.0.0",
        "apollo-upload-client": "^17.0.0",
        "big.js": "^6.1.1",
        "crypto-es": "^1.2.7",
        "express": "^4.17.1",
        "graphql": "^15.6.1",
        "lodash-es": "^4.17.21",
        "lodash.template": "^4.5.0",
        "ngx-progressbar": "^8.0.0",
        "rxjs": "~7.5.4",
        "striptags": "^3.2.0",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.5",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "14.0.2",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "~13.2.5",
        "@angular/compiler-cli": "~13.2.5",
        "@nguniversal/builders": "^13.0.2",
        "@playwright/test": "^1.15.2",
        "@types/apollo-upload-client": "^17.0.0",
        "@types/big.js": "^6.1.2",
        "@types/express": "^4.17.13",
        "@types/jasmine": "~3.10.3",
        "@types/jasminewd2": "~2.0.10",
        "@types/lodash-es": "^4.17.5",
        "@types/node": "^14.14.31",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "eslint": "^8.2.0",
        "jasmine-core": "~4.0.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "prettier": "2.5.1",
        "ts-node": "~10.6.0",
        "typescript": "~4.5.5"
    }
}
