{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build --prod && ng run artisans:server:production && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "browser": {
        "crypto": false
    },
    "dependencies": {
        "@angular/animations": "~11.2.0",
        "@angular/cdk": "~11.2.0",
        "@angular/common": "~11.2.0",
        "@angular/compiler": "~11.2.0",
        "@angular/core": "~11.2.0",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "~11.2.0",
        "@angular/localize": "^11.2.0",
        "@angular/material": "^11.2.0",
        "@angular/platform-browser": "~11.2.0",
        "@angular/platform-browser-dynamic": "~11.2.0",
        "@angular/platform-server": "^11.2.0",
        "@angular/router": "~11.2.0",
        "@apollo/client": "^3.3.11",
        "@ecodev/fab-speed-dial": "^7.0.0",
        "@ecodev/natural": "^33.0.0",
        "@graphql-tools/mock": "^8.0.0",
        "@nguniversal/express-engine": "^11.2.0",
        "apollo": "^2.32.1",
        "apollo-angular": "^2.3.0",
        "apollo-upload-client": "^14.1.3",
        "crypto-js": "^4.0.0",
        "decimal.js": "^10.2.1",
        "express": "^4.17.1",
        "graphql": "^15.5.0",
        "lodash-es": "^4.17.20",
        "lodash.template": "^4.5.0",
        "ngx-progressbar": "^6.1.0",
        "prosemirror-example-setup": "^1.1.2",
        "prosemirror-keymap": "^1.1.4",
        "prosemirror-schema-basic": "^1.1.2",
        "prosemirror-state": "^1.3.4",
        "prosemirror-view": "^1.17.6",
        "puppeteer": "^7.1.0",
        "rxjs": "~6.6.3",
        "striptags": "^3.1.1",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.0",
        "@angular/cli": "~11.2.0",
        "@angular/compiler-cli": "~11.2.0",
        "@nguniversal/builders": "^11.2.0",
        "@types/apollo-upload-client": "^14.1.0",
        "@types/express": "^4.17.11",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^14.14.37",
        "@types/prosemirror-model": "^1.11.3",
        "@types/prosemirror-schema-basic": "^1.0.2",
        "@types/prosemirror-schema-list": "^1.0.3",
        "@types/prosemirror-state": "^1.2.6",
        "@types/prosemirror-view": "^1.17.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "~6.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "2.2.1",
        "protractor": "~7.0.0",
        "ts-node": "~9.1.1",
        "tslint": "~6.1.0",
        "typescript": "~4.1.5"
    }
}
