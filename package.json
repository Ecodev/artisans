{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build && ng run artisans:server:production && find data/tmp/build -type f -not -name '*.gz' -exec gzip --keep --best {} \\; && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "dependencies": {
        "@angular/animations": "~12.0.2",
        "@angular/cdk": "~12.0.2",
        "@angular/common": "~12.0.2",
        "@angular/compiler": "~12.0.2",
        "@angular/core": "~12.0.2",
        "@angular/flex-layout": "^12.0.0-beta.34",
        "@angular/forms": "~12.0.2",
        "@angular/localize": "^12.0.2",
        "@angular/material": "^12.0.2",
        "@angular/platform-browser": "~12.0.2",
        "@angular/platform-browser-dynamic": "~12.0.2",
        "@angular/platform-server": "^12.0.2",
        "@angular/router": "~12.0.2",
        "@apollo/client": "^3.3.19",
        "@ecodev/fab-speed-dial": "^8.0.1",
        "@ecodev/natural": "^36.0.3",
        "@graphql-tools/mock": "^8.1.3",
        "@nguniversal/express-engine": "^12.0.0",
        "apollo": "^2.33.4",
        "apollo-angular": "^2.6.0",
        "apollo-upload-client": "^15.0.0",
        "crypto-es": "^1.2.7",
        "decimal.js": "^10.2.1",
        "express": "^4.17.1",
        "graphql": "^15.5.0",
        "lodash-es": "^4.17.21",
        "lodash.template": "^4.5.0",
        "ngx-progressbar": "^6.1.1",
        "prosemirror-example-setup": "^1.1.2",
