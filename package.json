{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "client-config": "`which php8.1 || which php` ./bin/generate-client-configuration.php",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn client-config && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build && ng run artisans:server:production && find data/tmp/build -type f -not -name '*.gz' -exec gzip --keep --best {} \\; && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint --max-warnings 0",
        "codegen": "./bin/dump-schema && apollo client:codegen --suppressDeprecationWarning -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "resolutions": {
        "graphql": "^15"
    },
    "dependencies": {
        "@angular/animations": "^15.1.2",
        "@angular/cdk": "^15.1.2",
        "@angular/common": "^15.1.2",
        "@angular/compiler": "^15.1.2",
        "@angular/core": "^15.1.2",
        "@angular/flex-layout": "^15.0.0-beta.42",
        "@angular/forms": "^15.1.2",
        "@angular/localize": "^15.1.2",
        "@angular/material": "^15.1.2",
        "@angular/platform-browser": "^15.1.2",
        "@angular/platform-browser-dynamic": "^15.1.2",
        "@angular/platform-server": "^15.1.2",
        "@angular/router": "^15.1.2",
        "@apollo/client": "^3.7.5",
        "@ecodev/fab-speed-dial": "^13.0.0",
        "@ecodev/natural": "^49.0.0",
        "@ecodev/natural-editor": "^49.0.0",
        "@graphql-tools/mock": "^8.7.16",
        "@nguniversal/express-engine": "^15.1.0",
        "apollo": "^2.34.0",
        "apollo-angular": "^4.2.0",
        "apollo-upload-client": "^17.0.0",
        "big.js": "^6.2.1",
        "crypto-es": "^1.2.7",
        "express": "^4.18.2",
        "graphql": "^16",
        "lodash-es": "^4.17.21",
        "lodash.template": "^4.5.0",
        "ngx-progressbar": "^9.0.0",
        "rxjs": "~7.8.0",
        "striptags": "^3.2.0",
        "tslib": "^2.5.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.1.3",
        "@angular-eslint/builder": "15.2.0",
        "@angular-eslint/eslint-plugin": "15.2.0",
        "@angular-eslint/eslint-plugin-template": "15.2.0",
        "@angular-eslint/schematics": "15.2.0",
        "@angular-eslint/template-parser": "15.2.0",
        "@angular/cli": "^15.1.3",
        "@angular/compiler-cli": "^15.1.2",
        "@nguniversal/builders": "^15.1.0",
        "@playwright/test": "^1.30.0",
        "@types/apollo-upload-client": "^17.0.2",
        "@types/big.js": "^6.1.6",
        "@types/express": "^4.17.16",
        "@types/jasmine": "~4.3.1",
        "@types/jasminewd2": "~2.0.10",
        "@types/lodash-es": "^4.17.5",
        "@types/node": "^16.0",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "eslint": "^8.33.0",
        "jasmine-core": "~4.5.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.4.1",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "prettier": "2.8.3",
        "ts-node": "~10.9.1",
        "typescript": "~4.9.5"
    }
}
