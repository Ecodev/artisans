        "karma": "~5.2.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "2.1.2",
        "protractor": "~7.0.0",
        "ts-node": "~9.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.3"
    }
}
