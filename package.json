{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build --prod && ng run artisans:server:production && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "browser": {
        "crypto": false
    },
    "dependencies": {
        "@angular/animations": "~10.1.2",
        "@angular/cdk": "~10.2.1",
        "@angular/common": "~10.1.2",
        "@angular/compiler": "~10.1.2",
        "@angular/core": "~10.1.2",
        "@angular/flex-layout": "^10.0.0-beta.32",
        "@angular/forms": "~10.1.2",
        "@angular/localize": "^10.1.2",
        "@angular/material": "^10.2.1",
        "@angular/platform-browser": "~10.1.2",
        "@angular/platform-browser-dynamic": "~10.1.2",
        "@angular/platform-server": "^10.1.2",
        "@angular/router": "~10.1.2",
        "@ecodev/fab-speed-dial": "^6.0.0",
        "@ecodev/natural": "^24.0.0",
        "@nguniversal/express-engine": "^10.1.0",
        "apollo": "^2.27.0",
        "apollo-angular": "^1.9.0",
        "apollo-angular-link-http": "^1.10.0",
        "apollo-angular-link-http-batch": "^1.8.0",
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link": "^1.2.13",
        "apollo-link-error": "^1.1.11",
        "apollo-link-schema": "^1.2.4",
        "apollo-upload-client": "^13.0.0",
        "crypto-js": "^4.0.0",
        "decimal.js": "^10.2.0",
        "express": "^4.17.1",
        "graphql": "^14.4.2",
        "graphql-tag": "^2.10.0",
        "graphql-tools": "^4.0.6",
        "lodash-es": "^4.17.15",
        "lodash.template": "^4.5.0",
        "ngx-avatar": "^4.0.0",
        "ngx-progressbar": "^6.0.3",
        "ngx-timeago": "^1.0.1",
        "prosemirror-example-setup": "^1.1.2",
        "prosemirror-keymap": "^1.1.4",
        "prosemirror-schema-basic": "^1.1.2",
        "prosemirror-state": "^1.3.3",
        "prosemirror-view": "^1.15.7",
        "puppeteer": "^5.3.0",
        "rxjs": "~6.6.3",
        "striptags": "^3.1.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1001.2",
        "@angular/cli": "~10.1.2",
        "@angular/compiler-cli": "~10.1.2",
        "@nguniversal/builders": "^10.1.0",
        "@types/express": "^4.17.8",
        "@types/jasmine": "~3.5.10",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash-es": "^4.17.3",
        "@types/node": "^12.12.31",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
