{
    "name": "artisans",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build --prod && ng run artisans:server:production && rm -rf htdocs/ && mv -v data/tmp/build htdocs",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts",
        "dev-ssr": "ng run artisans:serve-ssr",
        "serve-ssr": "node data/tmp/server/main.js",
        "prerender": "ng run artisans:prerender"
    },
    "browser": {
        "crypto": false
    },
    "dependencies": {
        "@angular/animations": "~9.1.1",
        "@angular/cdk": "~9.2.0",
        "@angular/common": "~9.1.1",
        "@angular/compiler": "~9.1.1",
        "@angular/core": "~9.1.1",
        "@angular/flex-layout": "^9.0.0-beta.29",
        "@angular/forms": "~9.1.1",
        "@angular/localize": "^9.1.1",
        "@angular/material": "^9.2.0",
        "@angular/platform-browser": "~9.1.1",
        "@angular/platform-browser-dynamic": "~9.1.1",
        "@angular/platform-server": "^9.1.1",
        "@angular/router": "~9.1.1",
        "@ecodev/fab-speed-dial": "^5.0.2",
        "@ecodev/natural": "^15.0.1",
        "@nguniversal/express-engine": "^9.1.0",
        "angular-file": "^3.0.1",
        "apollo": "^2.27.0",
        "apollo-angular": "^1.9.0",
        "apollo-angular-link-http": "^1.10.0",
        "apollo-angular-link-http-batch": "^1.8.0",
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link": "^1.2.13",
        "apollo-link-error": "^1.1.11",
        "apollo-link-schema": "^1.2.4",
        "apollo-upload-client": "^13.0.0",
        "core-js": "^3.6.5",
        "crypto-js": "^4.0.0",
        "decimal.js": "^10.2.0",
        "express": "^4.17.1",
        "graphql": "^14.4.2",
        "graphql-tag": "^2.10.0",
        "graphql-tools": "^4.0.6",
        "jsqr": "^1.1.1",
        "lodash": "^4.17.15",
        "lodash.template": "^4.5.0",
        "ngx-avatar": "^3.7.0",
        "ngx-progressbar": "^6.0.2",
        "ngx-timeago": "^1.0.1",
        "nyc": "^15.0.1",
        "prosemirror-example-setup": "^1.1.2",
        "prosemirror-keymap": "^1.1.3",
        "prosemirror-schema-basic": "^1.1.2",
        "prosemirror-state": "^1.3.3",
        "prosemirror-view": "^1.14.6",
        "puppeteer": "^2.0.0",
        "rxjs": "~6.5.5",
        "striptags": "^3.1.1",
        "tslib": "^1.11.1",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.1",
